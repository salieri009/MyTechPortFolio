{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
  "/images/*",
  "/css/*",
  "/js/*",
  "/assets/*",
  "/favicon.*",
  "/meta/*",
  "/*.css",
  "/*.js",
  "/*.mjs",
  "/*.map",
  "/*.ico",
  "/*.svg",
  "/*.png",
  "/*.jpg",
  "/*.gif",
  "/*.webp",
  "/*.json",
  "/assets/*.{css,js,mjs,map}"
    ]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html"
    },
    "401": {
      "rewrite": "/index.html"
    },
    "403": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript", 
    ".json": "application/json",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".ico": "image/x-icon"
  }
}
