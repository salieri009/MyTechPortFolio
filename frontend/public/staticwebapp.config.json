{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "*.js",
      "*.mjs",
      "*.css",
      "*.json",
      "*.ico",
      "*.svg",
      "*.png",
      "*.jpg",
      "*.gif",
      "*.woff",
      "*.woff2"
    ]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".json": "application/json"
  }
}
